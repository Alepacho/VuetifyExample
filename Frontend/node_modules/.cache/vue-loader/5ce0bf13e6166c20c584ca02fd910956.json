lue: 'course' },\n        ],\n        items: [],\n    }),\n    mounted () {\n        this.loadData();\n    },\n    methods: {\n        loadData() {\n            var self = this;\n            this.items = [];\n            \n            fetch(\"http://localhost:3000/api/\", {\n                //mode: 'no-cors',\n                method: 'GET'\n            })\n            .then(response => response.json())\n            .then((data) => {\n                console.log(data);\n                self.items = data;\n            }).catch(error => console.error(error));\n        }\n    }\n  }\n</script>\n"]}]}